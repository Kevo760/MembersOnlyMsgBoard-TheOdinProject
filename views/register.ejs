<!DOCTYPE html>
<html>
  <%- include('./partials/head.ejs') %>
  <%- include('./partials/nav.ejs') %>
  <body>
    <div class="content">
      <form method="post" action="" class="bg-dark" class="login-form">
        <h2>Register</h2>
        <input
        name="username"
        id="username"
        placeholder="Enter Username"
        type="text"
        required="true"
        >

        <input
        name="password"
        id="password"
        placeholder="Enter Password"
        type="text"
        required="true"
        >

        <input
        name="verifypassword"
        id="verifypassword"
        placeholder="Enter Password Again"
        type="text"
        required="true"
        >
        <button type="submit">Register</button>
      </form>
    </div>
  </body>
</html>